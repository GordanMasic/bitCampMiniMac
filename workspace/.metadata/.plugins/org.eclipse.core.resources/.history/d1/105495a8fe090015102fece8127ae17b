import java.util.Arrays;


public class Task1 {

	public static void main(String[] args) {
		
		int[] array = new int[100];
		int[] array1 = new int[]{1,2,3,4,5,6,7,8,9};	
		int i = 0;
		int j = 1;
		while (i < array.length){
			while(j<10){
				array[i] +=array1[j];
				
				
			}
			i++;
			j++;
		}System.out.println(Arrays.toString(array));

	}

}
