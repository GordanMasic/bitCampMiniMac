public class Place {

	public String name;

	/**
	 * Constructor for creating Place objects
	 * 
	 * @param name
	 *            initializing variable name
	 */
	public Place(String name) {
		this.name = name;
	}

	//Getters and setters
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

}
