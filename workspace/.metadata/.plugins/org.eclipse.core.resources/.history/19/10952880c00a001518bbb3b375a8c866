public class GetRandomNumber {

	public static int getRandomNumber(int min, int max) {
		
		int random = 0;
		if (max > min){
		random = (int)(Math.random()*max + min);
		}else{
			random = -1;
		}return random;
	}
	

	public static void main(String[] args) {

		System.out.println(getRandomNumber(-1, 4));

	}

}
