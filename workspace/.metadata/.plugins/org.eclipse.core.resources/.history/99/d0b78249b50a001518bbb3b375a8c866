
public class CanItBeBinary {

	
	public static boolean canBeBinary(int number){
		String num = Integer.toString(number, 10);
		boolean canBe = (Integer.toBinaryString(number) == num)?true:false;
		return canBe;
		
	}
	
	public static void main(String[] args) {
		System.out.println(canBeBinary(101));

	}

}
