import java.util.Arrays;


public class Task1 {

	public static void main(String[] args) {
		
		int[] array = new int[100];
		int j = 1;	
		int i = 0;
		while (i < array.length){
			while( j < 10 ){
				if (j<10){
				array[i] += j;
				j++;
				break;
				}
				else{
					j=0;
					break;
				}
				
			}i++;
		}System.out.println(Arrays.toString(array));

	}

}
