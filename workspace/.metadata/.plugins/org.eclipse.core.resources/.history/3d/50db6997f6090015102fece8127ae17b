
public class SumTwoNums {

	public static void main(String[] args) {
		
		
		int[] array1 = new int[]{1,2,3,4,5,6,7,8};
		int[] array2 = new int[]{1,2,1,2,1,2};
		
		int number1  = 0;
		int b = 1;
		for(int i = array1.length-1;i >= 0 ;i--){
			number1 += array1[i] * b;
			b *= 10;
		}
		System.out.println(number1);
	}

}
