import java.util.Arrays;


public class Task1 {

	public static void main(String[] args) {
		
		int[] array = new int[100];
			
		int i = 0;
		while (i < array.length){
			for(int j = i+1; j < 10; j++){
				if (j<10){
				array[i] += j;
				}
				else{
					break;
				}
				
				
			}i++;
		}System.out.println(Arrays.toString(array));

	}

}
