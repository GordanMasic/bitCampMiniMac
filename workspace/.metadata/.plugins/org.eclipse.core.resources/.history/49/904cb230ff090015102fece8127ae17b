import java.util.Arrays;


public class Task1 {

	public static void main(String[] args) {
		
		int[] array = new int[100];
		int[] array1 = new int[]{1,2,3,4,5,6,7,8,9};	
		int i = 0;
		while (i < array.length){
			for(int j = i+1; j < 10; j++){
				array[i] += j;
				break;
				
				
			}i++;
		}System.out.println(Arrays.toString(array));

	}

}
