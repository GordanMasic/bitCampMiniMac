
public class CanItBeBinary {

	
	public static boolean canBeBinary(int number){
		
		try{
			int bin = Integer.parseInt(number + "", 2);
			return true;
		}catch (NumberFormatException e){
		return false;
		}
	}
	
	public static void main(String[] args) {
		System.out.println(canBeBinary(25));

	}

}
